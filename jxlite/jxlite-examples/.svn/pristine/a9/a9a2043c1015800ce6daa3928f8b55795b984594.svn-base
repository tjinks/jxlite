package uk.co.cleopatra.xmlmap.examples;

import uk.co.cleopatra.xmlmap.annotations.Path;

public interface PersonWithMultiLineAddress {
	@Path("forename")
	String getForename();

	@Path("surname")
	String getSurname();

	@Path("@age")
	int getAge();
	
    @Path("address")
    MultiLineAddress getAddress();
}
